# Copy to .env and fill in your key from Google AI Studio:
# https://aistudio.google.com/app/apikey
GOOGLE_API_KEY=YOUR_KEY_HERE
# Optional: pick a default model (override per request if you like)
GEMINI_MODEL=gemini-2.5-flash

# Sanity.io credentials
# Get these from manage.sanity.io for your project
NEXT_PUBLIC_SANITY_PROJECT_ID="YOUR_PROJECT_ID"
NEXT_PUBLIC_SANITY_DATASET="development"
# This is a write token and should be kept secret. It's used for server-side operations.
SANITY_API_WRITE_TOKEN="YOUR_WRITE_TOKEN"
